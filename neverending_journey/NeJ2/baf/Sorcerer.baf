IF
	Clicked([ANYONE])
	Range(LastTrigger,10)
THEN
	RESPONSE #100
		SetGlobal("VP_InfoSwitch","GLOBAL",1)
		ActionOverride(Player1,StartDialog("Player1",Myself))
END

IF
	Clicked([ANYONE])
	!Range(LastTrigger,10)
THEN
	RESPONSE #100
		DisplayString(Myself,14702) // You are too far away to use that.
END


