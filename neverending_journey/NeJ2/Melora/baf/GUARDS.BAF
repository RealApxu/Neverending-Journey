IF
	OnCreation()
THEN
	RESPONSE #50
		StartTimer(1,300)
		SetGlobalTimer("DestroyGuards","GLOBAL",300)
	RESPONSE #50
		StartTimer(1,200)
		SetGlobalTimer("DestroyGuards","GLOBAL",200)
END

IF
	TimerExpired(1)
THEN
	RESPONSE #100
		DestroySelf()
END

IF
	GlobalTimerExpired("DestroyGuards","GLOBAL")
THEN
	RESPONSE #100
		DestroySelf()
END


